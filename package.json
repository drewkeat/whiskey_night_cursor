{
  "name": "whiskey-night",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "docker compose up -d && npx prisma migrate deploy && npx prisma db seed && next dev --webpack",
    "dev:no-seed": "docker compose up -d && npx prisma migrate deploy && next dev --webpack",
    "build": "next build --webpack",
    "start": "docker compose up -d && npx prisma migrate deploy && next start",
    "lint": "eslint",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "db:reset": "docker compose up -d && npx prisma migrate reset --force"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.2.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "prisma": "^5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "resend": "^6.9.1",
    "twilio": "^5.12.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
