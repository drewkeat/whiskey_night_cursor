{
  "name": "whiskey-night",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "docker compose up -d --wait && npx prisma generate && npx prisma migrate deploy && npx prisma db seed && next dev",
    "dev:no-seed": "docker compose up -d --wait && npx prisma migrate deploy && next dev",
    "build": "next build",
    "start": "docker compose up -d --wait && npx prisma migrate deploy && next start",
    "lint": "eslint",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "db:reset": "docker compose up -d --wait && npx prisma migrate reset --force",
    "db:reset-full": "docker compose down -v && docker compose up -d --wait && npx prisma migrate deploy && npx prisma db seed"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.2.0",
    "dotenv": "^17.2.4",
    "googleapis": "^171.4.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "openai": "^6.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "resend": "^6.9.1",
    "twilio": "^5.12.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@serwist/turbopack": "^9.5.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "esbuild": "^0.25.0",
    "esbuild-wasm": "^0.25.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^6.15.0",
    "serwist": "^9.0.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
